{
  "env": "development",
  "appName": "Wooster",
  "version": "1.0.0",
  "chatMode": "classic_tools",
  "logLevel": "info",
  "logging": {
    "consoleLogLevel": "info",
    "fileLogLevel": "info",
    "logFile": "logs/wooster_session.log",
    "logAgentLLMInteractions": false,
    "consoleQuietMode": true
  },
  "codeAgent": {
    "maxAttempts": 2,
    "stepTimeoutMs": 20000,
    "totalTimeoutMs": 60000,
    "memoryLimitMb": 128,
    "maxOutputLength": 10000,
    "logging": {
      "enabled": true,
      "redactions": true
    }
  },
  "openai": {
    "apiKey": "YOUR_OPENAI_API_KEY_HERE",
    "modelName": "gpt-4o-mini",
    "embeddingModelName": "text-embedding-3-small",
    "temperature": 0.7,
    "maxTokens": 2048
  },
  "tavily": {
    "apiKey": null
  },
  "google": {
    "calendar": {
      "clientId": null,
      "clientSecret": null,
      "refreshToken": null,
      "calendarId": "primary",
      "defaultAttendeeEmail": null
    }
  },
  "userProfile": {
    "enabled": true,
    "storePath": "./vector_data/user_profile_store"
  },
  "gtd": {
    "basePath": "./gtd",
    "projectsDir": "./projects",
    "archiveDir": "./gtd/archive",
    "nextActionsArchiveDirPath": "./gtd/archive/nextActions",
    "inboxPath": "./gtd/inbox.md",
    "nextActionsPath": "./gtd/next_actions.md",
    "nextActionsViewFormat": "list",
    "somedayMaybePath": "./gtd/someday_maybe.md",
    "waitingForPath": "./gtd/waiting_for.md"
  },
  "gmail": {
    "senderEmailAddress": null,
    "userPersonalEmailAddress": null,
    "emailAppPassword": null
  },
  "weather": {
    "city": null,
    "openWeatherMapApiKey": null,
    "units": "F"
  },
  "dailyReview": {
    "scheduleCronExpression": "30 6 * * *"
  },
  "captureApi": {
    "enabled": false,
    "port": 3002,
    "apiKey": null,
    "ipWhitelistEnabled": false,
    "allowedIps": []
  },
  "apiPlugin": {
    "enabled": false,
    "port": 3000,
    "apiKey": null,
    "globalIpWhitelistEnabled": false,
    "globalAllowedIps": []
  },
  "personalHealth": {
    "healthDir": "./health"
  },
  "routing": {
    "enabled": true,
    "strategy": "speed",
    "fallbackChain": ["gpt-4o-mini", "gpt-4o"],
    "providers": {
      "openai": {
        "enabled": true,
        "models": {
          "fast": "gpt-4o-mini",
          "quality": "gpt-4o"
        },
        "rateLimiting": false,
        "costTracking": false
      },
      "local": {
        "enabled": true,
        "serverUrl": "http://127.0.0.1:8080",
        "autoStart": false,
        "models": {
          "fast": "mlx-community/Qwen3-14B-4bit-AWQ"
        },
        "embeddings": {
          "enabled": false,
          "serverUrl": "http://127.0.0.1:8081/v1",
          "projects": {
            "enabled": false,
            "model": "Qwen/Qwen3-Embedding-4B",
            "dimensions": 2560
          },
          "userProfile": {
            "enabled": false,
            "model": "Qwen/Qwen3-Embedding-4B",
            "dimensions": 2560
          }
        }
      }
    },
    "profiles": {},
    "healthCheck": {
      "interval": 30000,
      "timeout": 5000,
      "retries": 3
    },
    "logging": {
      "decisions": false,
      "performance": false,
      "errors": true
    }
  },
  "plugins": {
    "projectManager": {
      "enabled": true
    },
    "frontend": {
      "enabled": true,
      "port": 3000
    },
    "signal": {
      "enabled": false
    }
  }
} 